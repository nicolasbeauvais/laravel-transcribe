{
    "name": "nicolasbeauvais/laravel-transcribe",
    "description": "A simple language files translation manager in your artisan console",
    "keywords": ["laravel","laravel-framework","translation","translate","i18n","localization","multilingual","console"],
    "homepage": "https://github.com/nicolasbeauvais/laravel-transcribe",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Beauvais",
            "email": "nicolasbeauvais1@gmail.com",
            "homepage": "http://nicolas-beauvais.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/console": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/filesystem": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "NicolasBeauvais\\Transcribe\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NicolasBeauvais\\Transcribe\\TranscribeServiceProvider"
            ]
        }
    }
}
